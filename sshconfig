ForwardAgent yes
Compression yes
#ConnectionAttempts 5
ForwardX11 yes
HashKnownHosts no
GSSAPIDelegateCredentials yes

ServerAliveInterval 60

Host *
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%h:%p
    ServerAliveInterval 60
    ServerAliveCountMax 5

Include ~/.ssh/customer_ssh_config
